{
  "name": "navigation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pm2:dev": "pm2 start npm --name navigation-user-front -- run dev -- -p 4000",
    "pm2:prod": "pm2 start npm --name navigation-user-front -- run start -- -p 3000",
    "pm2:prod:local": "next build && pm2 start npm --name navigation-user-front -- run start -- -p 3000",
    "qa:local": "env-cmd -f .env.development.local next dev -p 3030",
    "qa:build": "env-cmd -f .env.development next build",
    "qa:start": "env-cmd -f .env.development next start -p 3030",
    "live:local": "env-cmd -f .env.production.local next -p 3030",
    "live:build": "env-cmd -f .env.production next build",
    "live:start": "env-cmd -f .env.production next start -p 3030"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.4.3",
    "@tanstack/react-query-devtools": "^5.4.3",
    "axios": "^1.6.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "next": "13.5.4",
    "next-auth": "^4.24.4",
    "react": "^18",
    "react-dom": "^18",
    "react-lottie-player": "^1.5.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "autoprefixer": "^10",
    "eslint": "^8.52.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3",
    "typescript": "^5.2.2"
  }
}
